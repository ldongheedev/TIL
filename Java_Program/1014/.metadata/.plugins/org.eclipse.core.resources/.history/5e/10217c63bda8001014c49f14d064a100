package sec01_1;

public class Ticket {
	protected int number;
	protected double price;
	
	public Ticket (int number) {
		this.number = number;
	}

	public int getNumber() {
		return number;
	}

	public double getPrice() {
		return price;
	}

	public String toString() {
		return "번호: " + number + ", 가격: " + price;
	}
	
	public void setPrice(double price) {
		this.price = price;
	}
}
