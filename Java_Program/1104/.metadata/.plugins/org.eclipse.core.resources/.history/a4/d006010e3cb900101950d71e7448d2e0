package sec01;

import javax.swing.*; //클래스, 인터페이스 포함, 하위 패키지는 포함하지 않음

public class EventTest1 extends JFrame{
	
	//멤버변수
	JButton btn1;
	
	//생성자
	EventTest1() {
		setTitle("독립된 클래스로 이벤트 리스너 만들기");
		setSize(300,300);
		setVisible(true);
	}

	public static void main(String[] args) {
		new EventTest1();
		
	}

}
